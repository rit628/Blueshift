task add(int intResult, float floatResult) {
    int a = 9;
    int b = 10;
    intResult = a + b + 2;
    floatResult = a + b + 2.1;
}

task subtract(int intResult, float floatResult) {
    int a = 9;
    int b = 10;
    intResult = a - b;
    floatResult = a - b - 5.28;
}

task multiply(int intResult, float floatResult) {
    int a = 9;
    int b = 10;
    intResult = a * b;
    floatResult = a * b * .5;
}

task divide(int intResult, float floatResult) {
    int a = 9;
    int b = 10;
    intResult = a / b;
    floatResult = a / b;
}

task grouped(int intResult, float floatResult) {
    intResult = 10 * (8 - 9);
    floatResult = 10 * 8 - 9;
}

setup() {
    virtual int intResult = 0;
    virtual float floatResult = 0;
    add(intResult, floatResult);
    subtract(intResult, floatResult);
    multiply(intResult, floatResult);
    divide(intResult, floatResult);
    grouped(intResult, floatResult);
}