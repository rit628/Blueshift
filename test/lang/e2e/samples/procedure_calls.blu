int exponentiate(int a, int b) {
    return a ^ b;
}

task simpleCall(int result) {
    result = exponentiate(2, 3);
}

task compoundCall(int result) {
    result = exponentiate(exponentiate(2, 3), exponentiate(2, 1));
}

setup() {
    virtual int result = 0;
    simpleCall(result);
    compoundCall(result);
}