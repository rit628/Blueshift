oblock receiver(LINE_WRITER d1, LINE_WRITER d2, FILE_LOG d3)
: triggerOn(d1, d2)
{
    d3.add_msg = d1.msg + d2.msg; 
   
}

setup(){
    LINE_WRITER dev1 = "CTL1::file-file1.txt"; 
    LINE_WRITER dev2 = "CTL1::file-omar.txt"; 
    FILE_LOG dev3 = "CTL1::file-logfile.txt"; 
    receiver(dev1, dev2, dev3);
}
