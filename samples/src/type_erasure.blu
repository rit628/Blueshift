oblock test(KEYBOARD kbd) {
    any json = loadJson("{\"a\" : 2.4}");
    print(json);
    print(jsonify(json));
    if (containsType(json, "map")) {
        print(containsType(json["a"], "float"));
        print(json["a"] + 17);
    }
}

setup() {
    KEYBOARD KBD = "ctl::mode-all";
    test(KBD);
}